/* pages/store/list/list.wxss */

page {
  background-color: #f6f6f6;
}

.none {
  display: none;
}

.block {
  display: block;
}

.listTop {
  width: 100%;
  height: 89rpx;
  border-bottom: 1rpx solid #e7e7e7;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 99;
  background-color: #fff;
}

.listTop .li {
  width: 249rpx;
  height: 35rpx;
  border-right: 1rpx solid #e7e7e7;
  float: left;
  text-align: center;
  font-size: 30rpx;
  line-height: 35rpx;
  margin-top: 27.5rpx;
  position: relative;
}

.listTop .select {
  color: #11a8e1;
}

.listTop .li:last-child {
  border: 0;
}

.listTop .li image {
  width: 22rpx;
  height: 12rpx;
  position: absolute;
  left: 190rpx;
  top: 13rpx;
}

.listTop .li .img2 {
  display: none;
}

.listTop .select .img2 {
  display: block;
}

.listTop .select .img1 {
  display: none;
}

.listToplayer {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 90rpx;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 999;
}

.listToplayer .li {
  padding: 0 20rpx;
  border-bottom: 1px solid #e7e7e7;
  height: 99rpx;
  line-height: 99rpx;
  background-color: #fff;
  font-size: 30rpx;
  color: #333;
}

.listToplayer .select {
  color: #11a8e1;
}

.listToplayeranment {
  animation: 0.3s linear 0s rotate;
  animation-fill-mode: forwards;
  -webkit-transition: 0.3s linear 0s rotate;
  -moz-transition: 0.3s linear 0s rotate;
  -o-transition: 0.3s linear 0s rotate;
}

@keyframes rotate {
  from {
    transform: translateY(-100%);
    opacity: 0;
  }

  to {
    transform: translateY(0);
    opacity: 1;
  }
}

/* screen */

.clear {
  clear: both;
}

.screen {
  background-color: #fff;
}

.screen-box {
  padding: 30rpx 30rpx 0rpx 30rpx;
}

.screen-title {
  font-size: 30rpx;
  line-height: 30rpx;
  color: #333;
  margin-bottom: 20rpx;
}

.screen-tag {
  float: left;
  font-size: 26rpx;
  line-height: 46rpx;
  padding: 10rpx 24rpx;
  border: 1rpx solid #767676;
  border-radius: 5rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}

.screen .select {
  background-color: #11a8e1;
  color: #fff;
  border: 1rpx solid rgba(255, 0, 0, 0);
}

.screen-btn {
  height: 110rpx;
  margin-top: 20rpx;
}

.reset, .confirm {
  text-align: center;
  display: flex;
  justify-content: center;
  flex-direction: column;
  font-size: 36rpx;
}

.reset {
  width: 50%;
  height: 110rpx;
  float: left;
  border-top: 1rpx solid #e7e7e7;
}

.confirm {
  width: 50%;
  height: 110rpx;
  float: left;
  background-color: #11a8e1;
  color: #fff;
  border-top: 1rpx solid #11a8e1;
}

/* list */

.left {
  float: left;
}

.right {
  float: right;
}

.clear {
  clear: both;
}

#list {
  margin-top: 89rpx;
}

.list-item {
  height: 247rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}

.list-box {
  padding: 30rpx;
}

.list-img {
  width: 210rpx;
  height: 187rpx;
  padding-right: 30rpx;
  float: left;
}

.list-img image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.list-info {
  float: left;
  width: calc(100% - 240rpx);
  margin-top: 10rpx;
}

.list-title {
  height: 30rpx;
}

.list-name {
  font-size: 30rpx;
  line-height: 30rpx;
  color: #333;
}

.list-name-img {
  position: relative;
  width: 35rpx;
  height: 30rpx;
  margin-left: 18rpx;
}

.list-name-img image {
  width: 35rpx;
  height: 30rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

.list-evaluate {
  height: 24rpx;
  margin: 18rpx 0rpx;
}

.list-evaluate-left, .list-evaluate-right {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #aeaeae;
}

.list-evaluate-left text, .list-evaluate-right text {
  color: #ff7979;
}

.list-evaluate-line {
  width: 51rpx;
  height: 24rpx;
  font-size: 24rpx;
  line-height: 24rpx;
  text-align: center;
  color: #c8c8c8;
}

.list-address {
  height: 24rpx;
  font-size: 24rpx;
  line-height: 24rpx;
  color: #aeaeae;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.list-more {
  height: 26rpx;
  margin-top: 20rpx;
}

.list-tag {
  width: 42rpx;
  font-size: 16rpx;
  line-height: 26rpx;
  color: #11a8e1;
  border: 1rpx solid #11a8e1;
  text-align: center;
  border-radius: 6rpx;
  margin-right: 10rpx;
}

.list-distance {
  font-size: 26rpx;
  line-height: 26rpx;
  color: #aeaeae;
}

.NoneSj{
  width: 100%;
  height: 100%;
  position: fixed;
  background-color: #fff;
  z-index: 98;
  left: 0;
  top: 130rpx;
}
.NoneSj image{
  width: 300rpx;
  height: 300rpx;
  position: absolute;
  left: 50%;
  top: 20%;
  margin: 0 0 0 -150rpx;
}