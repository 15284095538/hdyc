<!--我的资料-->
<view class='myInfo'>
<view class='input'>
<view class='word'>头像</view>
<image src='{{user.pic}}'></image>
</view>
<view class='input'>
<view class='word'>真实姓名</view>
<input placeholder='请输入真实姓名' value='{{user.zname}}'></input>
</view>
<view class='input'>
<view class='word'>手机号</view>
<view class='zname'>{{user.tel}}</view>
</view>
<view class='input'>
<view class='word'>性别</view>
<view class='sex'>
<radio checked="{{user.sex =='男'?'true':''}}"disabled="true">男 </radio>
<radio checked="{{user.sex=='女'?'true':''}}"disabled="true">女 </radio>
</view>

</view>
<view class='input'>
<view class='word'>昵称</view>
<view class='zname'>{{user.usname}}</view>
</view>

</view>
