/* pages/my/info/integral/list/list.wxss */
.list{
  padding: 10rpx 30rpx 0;
  color: #333;
  font-size: 28rpx;
}
.list .li{
  padding: 30rpx 0;
  height: 150rpx;
  border-bottom: 1px solid #f6f6f6;
}
.list .li .img{
  width: 150rpx;
  height: 150rpx;
  float: left;
}
.list .li .img image{
  width: 150rpx;
  height: 150rpx;
}
.list .li .text{
  float: right;
  width: 500rpx;
}
.list .li .name{
  height: 50rpx;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.list .li .p{
  color: #aeaeae;
  font-size: 24rpx;
  height: 50rpx;
  line-height: 50rpx;
}
.list .li .price{
  color: #ff7979;
  margin-top: 20rpx;
  
}
.list .li .flex{
  float: right;
  color: #aeaeae;
}
.dorl{
  margin-top: 10rpx;
}