.NoneSj{
  width: 100%;
  height: 70%;
  position: fixed;
  background-color: #fff;
  z-index: 98;
  left: 0;
  top: 130rpx;
}
.NoneSj image{ 
  width: 300rpx;
  height: 300rpx;
  position: absolute;
  left: 50%;
  top: 30%;
  margin: 0 0 0 -150rpx;
}
.mycars .cart_container {
  display: flex;
  flex-direction: row;
  height: 120rpx;
  border-bottom: 1px solid #aeaeae;
}
.mycars{
  padding-bottom: 200rpx;
}
.mycars .scroll {
  padding-bottom: 120rpx;
}

.mycars .column {
  display: flex;
  width: 422rpx;
  margin-left: 20rpx;
  flex-direction: column;
}

.mycars .row {
  display: flex;
  flex-direction: row;
  font-size: 26rpx;
  align-items: center;
}

.mycars .sku {
  margin-top: 60rpx;
  margin-left: 100rpx;
}

.mycars .sku-price {
  color: red;
  position: relative;
  margin-top: 40rpx;
}

.mycars .price {
  color: red;
  position: relative;
}

.mycars .title {
  width: 400rpx;
  font-size: 28rpx;
  line-height: 120rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.mycars .xq {
  font-size: 26rpx;
  color: #aeaeae;
  margin-top: 10rpx;
}

.mycars .small_text {
  font-size: 28rpx;
  margin-right: 40rpx;
  margin-left: 10rpx;
}

.mycars .item-select {
  width: 40rpx;
  height: 40rpx;
  margin-top: 90rpx;
  margin-left: 40rpx;
}

.mycars .item-allselect {
  width: 40rpx;
  height: 40rpx;
  margin-left: 20rpx;
}

.mycars .item-image {
  width: 80rpx;
  height: 80rpx;
  margin: 20rpx 20rpx 20rpx 40rpx;
}

.mycars .bottom_line {
  width: 100%;
  height: 2rpx;
  background: lightgray;
}

.mycars .bottom_total {
  position: fixed;
  display: flex;
  flex-direction: column;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
  background: white;
}

.mycars .button-red {
  background-color: #11a8e1; /* 红色 */
}

.mycars .hj {
  height: 55rpx;
  margin-top: -20rpx;
}

.mycars .js {
  height: 55rpx;
  margin-top: -10rpx;
}

.mycars .navbar {
  display: flex;
  flex-direction: row;
  line-height: 80rpx;
}

.mycars .navbar .item {
  flex: auto;
  font-size: 30rpx;
  text-align: center;
  background: #fff;
}

.mycars .navbar .item.active {
  color: #11a8e1;
  position: relative;
  border-bottom: 5rpx solid #11a8e1;
}

.mycars .navbar .item.active::after {
  display: block;
  position: absolute;
  height: 4rpx;
  bottom: 0;
  left: 0;
  right: 0;
  background: #11a8e1;
}

.mycars .yg {
  color: red;
  width: 200rpx;
  position: relative;
  top: 20rpx;
  font-size: 26rpx;
  left: 20rpx;
}

.mycars .row1 {
  width: 500rpx;
}

.mycars .del {
  background-color: #e64340;
  width: 65rpx;
  z-index: 4;
  right: 0;
  color: #fff;
  display: none;
  padding: 45rpx 30rpx;
  font-size: 30rpx;
}
.mycars .mrcar{
  background-color: #11a8e1;
}
.button{
  position: fixed;
  top: 1065rpx;
  left: 30rpx;
  width: 642rpx;
  text-align: center;
  background: #11a8e1;
  color: #fff;
  padding: 20rpx;
  border-radius: 10rpx;
}
