<view class="carlove">
<view class='bg'>
<swiper class='swiper' indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" previous-margin="88rpx"next-margin="88rpx">
      <block wx:for="{{cars}}" wx:key="this" wx:for-item="item">
        <swiper-item style="width:493rpx">
          <view class='car'>
          <view class='carname'>
          <image src='{{item.img}}'></image>
          </view>
          <view class='ping'>{{item.name}}</view>
          <view class='kuan'>{{item.kuan}}</view>
          <view class="mr " hidden='{{item.mr!==1}}'>已设为默认</view>
          </view>
        </swiper-item>
      </block>
    </swiper>
    </view>

<view class='button'>我的车库</view>
</view>