/* pages/my/home/index.wxss */

.row {
  display: block;
  margin: 0px;
}

.col {
  display: flex;
  font-family: -apple-system-font, "Helvetica Neue", sans-serif;
  font-size: 17px;
  height: 100%;
}

.col>.col-1, .col-2, .col-3, .col-4, .col-5, .col-6, .col-7, .col-8, .col-9,
.col-10, .col-11, .col-12 {
  overflow: hidden;
}
.col-6 {
  width: 50%;
  height: 100%;
  text-align: center;
}
.col-3 {
  width: 25%;
  height: 100%;
  text-align: center;
}

.left {
  float: left;
}

.right {
  float: right;
}

.clear {
  clear: both;
}

.indent {
  justify-content: center;
  flex-direction: column;
  position: relative;
  overflow: hidden;
}

page {
  background-color: #f6f6f6;
}

/* user */

#user {
  position: relative;
  padding-bottom: 50rpx;
}

#header {
  height: 300rpx;
  width: 100%;
  background-color: #11a8e1;
  margin-bottom: 90rpx;
}

.info-box {
  height: 130rpx;
  padding: 30rpx;
  padding-top: 75rpx;
}

.info-img {
  height: 130rpx;
  width: 130rpx;
  margin-right: 30rpx;
  border-radius: 50%;
}

.info-img image {
  width: 100%;
  height: 100%;
  border: 4rpx solid #fff;
  border-radius: 50%;
}

.info-infos {
  height: 130rpx;
  width: calc(100% - 160rpx);
}

.info-name {
  width: 100%;
  height: 65rpx;
  font-size: 36rpx;
  letter-spacing: 2rpx;
  color: #fff;
  line-height: 65rpx;
}

.info-code {
  width: 284rpx;
  margin-top: 6.5rpx;
  height: 52rpx;
  background-color: #4481b6;
  border-radius: 25rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  position: relative;
  overflow: hidden;
}

.info-codeimg {
  width: 52rpx;
  height: 52rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

.info-codeimg image {
  width: 22rpx;
  height: 22rpx;
  padding: 15rpx;
}

.info-codetext {
  width: 232rpx;
  height: 52rpx;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  line-height: 52rpx;
  white-space: nowrap;
  font-size: 22rpx;
  letter-spacing: 1rpx;
  color: #fff;
}

/* archives */

#archives {
  width: 690rpx;
  height: 125rpx;
  background-color: #fff;
  border-radius: 22rpx;
  margin-left: calc((100% - 690rpx) / 2);
  margin-top: 5rpx;
}

.archives-box {
  height: 79rpx;
  border-radius: 22rpx;
  padding: 23rpx 30rpx;
  position: relative;
}

.archives-title {
  height: 79rpx;
  width: 136rpx;
  border-right: 1rpx solid #efefef;
  margin-right: 48rpx;
  position: absolute;
  top: 23rpx;
  left: 30rpx;
}

.archives-title-text {
  font-size: 24rpx;
  line-height: 40rpx;
  letter-spacing: 1rpx;
  color: #11a8e1;
  height: 40rpx;
  text-align: center;
  margin-top: 5rpx;
}

.archives-title-line {
  height: 12rpx;
  margin-top: 10rpx;
}

.archives-line {
  width: 32rpx;
  height: 2rpx;
  background-color: #11a8e1;
  border-radius: 1rpx;
  margin: 2rpx 52rpx;
}

.archives-info {
  height: 99rpx;
  width: 446rpx;
  position: absolute;
  top: 13rpx;
  right: 30rpx;
}

.archives-info-img {
  width: 71rpx;
  height: 71rpx;
  padding: 8rpx 4rpx 0 0;
  margin-right: 21rpx;
}

.archives-info-img image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.archives-info-box {
  width: 346rpx;
  height: 99rpx;
}

.archives-info-title {
  width: 346rpx;
  height: 40rpx;
}

.archives-title-name {
  width: 350rpx;
  font-size: 24rpx;
  line-height: 50rpx;
  letter-spacing: 1rpx;
  color: #484848;
   white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis; 
}

.archives-info-subtitle {
  width: 346rpx;
  height: 39rpx;
  font-size: 18rpx;
  line-height: 39rpx;
  letter-spacing: 1rpx;
  color: #8d8d8d;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/* order */

#order {
  width: 690rpx;
  height: 280rpx;
  background-color: #fff;
  border-radius: 22rpx;
  margin-left: calc((100% - 690rpx) / 2);
}

.order-title {
  height: 79rpx;
  border-bottom: 1rpx solid #e7e7e7;
}

.order-title-text {
  font-size: 30rpx;
  line-height: 79rpx;
  letter-spacing: 2rpx;
  color: #333;
  margin-left: 30rpx;
}

.order-title-more {
  font-size: 26rpx;
  line-height: 79rpx;
  letter-spacing: 2rpx;
  color: #939393;
  margin-right: 30rpx;
}

.order-more-img {
  width: 14rpx;
  height: 26rpx;
  margin-left: 20rpx;
  margin-top: 26.5rpx;
  position: relative;
}

.order-more-img image {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

.order-menu {
  height: 90rpx;
  padding: 55rpx 30rpx;
}

.order-menu-img {
  width: 48rpx;
  height: 48rpx;
  margin-left: calc((100% - 48rpx) / 2);
}

.order-menu-img image {
  width: 100%;
  height: 100%;
}

.order-menu-text {
  width: 100%;
  height: 24rpx;
  font-size: 24rpx;
  line-height: 24rpx;
  letter-spacing: 1rpx;
  color: #6d6d6d;
  margin-top: 18rpx;
}

/* more */

.max {
  width: 100%;
  height: 100%;
}

#more {
  width: 690rpx;
  height: 700rpx;
  background-color: #fff;
  border-radius: 22rpx;
  margin-left: calc((100% - 690rpx) / 2);
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.more-list {
  height: 39rpx;
  border-bottom: 1rpx solid #e7e7e7;
  padding: 30rpx;
}

.border-none {
  border-bottom: 1rpx solid rgba(0, 0, 0, 0);
}

.more-list-img {
  width: 39rpx;
  height: 39rpx;
}

.more-list-text {
  height: 39rpx;
  font-size: 30rpx;
  line-height: 42rpx;
  letter-spacing: 2rpx;
  color: #333;
  margin-left: 30rpx;
}

.more-title-more {
  height: 39rpx;
  font-size: 30rpx;
  line-height: 42rpx;
  letter-spacing: 2rpx;
  color: #939393;
}

.more-title-more .number {
  color: #11a8e1;
}

.more-more-img {
  width: 14rpx;
  height: 26rpx;
  margin-left: 20rpx;
  margin-top: 8rpx;
  position: relative;
}

.more-more-img image {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

.more-title-more .dian {
  font-size: 39px;
  color: #ff7979;
}

/* business */

#business {
  position: relative;
  padding-bottom: 50rpx;
}

.archives-business-img {
  width: 35rpx;
  height: 35rpx;
  padding: 22rpx;
  margin-right: 21rpx;
  position: relative;
}

.archives-business-img image {
  width: 35rpx;
  height: 35rpx;
  position: absolute;
  top: 22rpx;
  left: 22rpx;
}

.archives-info-balance {
  height: 79rpx;
  line-height: 79rpx;
  font-size: 48rpx;
  letter-spacing: -1rpx;
  color: #11a8e1;
  max-width: 224rpx;
}

.archives-info-balance text {
  font-size: 22rpx;
  font-weight: normal;
  font-stretch: normal;
  line-height: 48rpx;
  letter-spacing: 0rpx;
  color: #5c5c5c;
}

.archives-info-withdraw {
  width: 122rpx;
  height: 45rpx;
  background-color: rgba(17, 168, 225, 0.9);
  border-radius: 22rpx;
  font-size: 22rpx;
  line-height: 45rpx;
  letter-spacing: 2rpx;
  color: #fff;
  text-align: center;
  margin-top: 17rpx;
}
/* order */
.order-number{
  font-size: 20rpx;
	line-height: 48rpx;
	letter-spacing: 0rpx;
	color: #5c5c5c;
}
.order-number text{
  font-size: 45rpx;
	line-height: 48rpx;
	letter-spacing: 0rpx;
	color: #11a8e1;
}
.order-text{
  font-size: 24rpx;
	line-height: 48rpx;
	letter-spacing: 0rpx;
	color: #6d6d6d;
}
/* more */
#archivesmore {
  width: 690rpx;
  height: 400rpx;
  background-color: #fff;
  border-radius: 22rpx;
  margin-left: calc((100% - 690rpx) / 2);
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}


.Userinfo{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
  z-index: 9999;
}