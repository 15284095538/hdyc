/* pages/my/business/order/order.wxss */

page {
  background-color: #f6f6f6;
}

.max {
  width: 100%;
  height: 100%;
}

.left {
  float: left;
}

.right {
  float: right;
}

.clear {
  clear: both;
}

.indent {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/* order */

.order-list {
  height: 267rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}

.order-title {
  height: 30rpx;
  padding: 35rpx 30rpx;
  border-bottom: 1rpx solid #e7e7e7;
}

.order-ns {
  font-size: 30rpx;
  line-height: 30rpx;
  letter-spacing: 0px;
  color: #aeaeae;
}

.order-status {
  font-size: 30rpx;
  line-height: 30rpx;
  color: #11a8e1;
}
