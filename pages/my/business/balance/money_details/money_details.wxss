}

.max {
  width: 100%;
  height: 100%;
}

.left {
  float: left;
}

.right {
  float: right;
}

.clear {
  clear: both;
}

.indent {
  justify-content: center;
  flex-direction: column;
  position: relative;
  overflow: hidden;
}

.list {
  height: 135rpx;
  background-color: #fff;
  padding: 0rpx 30rpx;
}

.list-box {
  height: 89rpx;
  padding: 22rpx 0rpx;
  border-bottom: 1rpx solid #e7e7e7;
}

.border-none {
  border-bottom: 1rpx solid rgba(0, 0, 0, 0);
}

.list-box-img {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 30rpx;
}

.list-box-info {
  height: 90rpx;
}

.list-info-name {
  height: 45rpx;
  font-size: 30rpx;
  line-height: 45rpx;
  letter-spacing: 0rpx;
  color: #333;
}

.list-info-time {
  height: 45rpx;
  font-size: 26rpx;
  line-height: 45rpx;
  letter-spacing: 0rpx;
  color: #aeaeae;
}

.list-box-info {
  height: 90rpx;
  font-size: 30rpx;
  line-height: 90rpx;
