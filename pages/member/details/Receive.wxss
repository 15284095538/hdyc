page{
  color: #333;
  font-size: 28rpx;
}
.info{
  padding: 25rpx;
  overflow: hidden;
  border-bottom: 1rpx solid #e7e7e7;
}
.info .top text:first-child{
  margin-right: 30rpx;
}
.info .address{
  margin-top: 20rpx;
}

.type{
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 25rpx;
  border-bottom: 1rpx solid #e7e7e7;
}
.type .fl{
  float: left;
}
.type .fr{
  float: right;
}
.type image{
  width: 14rpx;
  height: 26rpx;
  float: right;
  margin: 43rpx 0 0 0;
}

.menberBtn{
  width: 690rpx;
  height: 90rpx;
  background-color: #11a8e1;
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  border-radius: 10rpx;
  overflow: hidden;
  margin: 25rpx auto 0;
  line-height: 90rpx;
}