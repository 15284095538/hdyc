.none{
  display: none;
}
.block{
  display: block;
}

.indSwiper{
  width: 100%;
  height: 420rpx;
}
.indSwiper .slide-image{
  width: 100%;
  height: 420rpx;
}
.detalisTop{
  padding: 0 30rpx;
  border-bottom: 25rpx solid #f6f6f6;
}
.detalisTop .price{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
}
.detalisTop .price .p{
  color: #ff7979;
  width: 470rpx;
  float: left;
}

.detalisTop .price .r{
  width: 188rpx;
  height: 37rpx;
  position: relative;
  float: right;
  margin-top: 20rpx;
}
.detalisTop .price .r image{
  width: 188rpx;
  height: 37rpx;
  display: block;
  margin: 0;
}
.detalisTop .price .r text{
  width: 188rpx;
  height: 37rpx;
  line-height: 37rpx;
  position: absolute;
  left: 0;
  top: 0;
  text-align: center;
  font-size: 22rpx;
  color: #fff;
}
.detalisTop .name{
  height: 40rpx;
  line-height: 40rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
}
.detalisTop .det{
  position: relative;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  color: #aeaeae;
}
.detalisTop .det .l{
  position: absolute;
  left: 0;
  top: 0;
}
.detalisTop .det .c{
  width: 300rpx;
  position: absolute;
  left: 50%;
  top: 0;
  margin-left: -150rpx;
  text-align: center;
}
.detalisTop .det .r{
  position: absolute;
  right: 0;
  top: 0;
}
.detalisbody{
  padding: 0 30rpx;
}
.detalisbody .li{
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1rpx solid #e7e7e7;
  font-size: 28rpx;
  color: #333;
}
.detalisbody .li .l{
  width: 400rpx;
  height: 100rpx;
  float: left;
}
.detalisbody .li .c{
  float: right;
}
.detalisbody .li .r{
  width: 17rpx;
  height: 33rpx;
  float: right;
  margin: 33.5rpx 0 0 30rpx;
}
.detalisbody .li .r image{
  width: 17rpx;
  height: 33rpx;
  display: block;
}
.detalisbody .li:last-child .c{
  width: 188rpx;
  height: 37rpx;
  position: relative;
  float: right;
  margin-top: 30rpx;
}
.detalisbody .li:last-child .c image{
  width: 188rpx;
  height: 37rpx;
  display: block;
  margin: 0;
}
.detalisbody .li:last-child .c text{
  width: 188rpx;
  height: 37rpx;
  line-height: 37rpx;
  position: absolute;
  left: 0;
  top: 0;
  text-align: center;
  font-size: 22rpx;
  color: #fff;
}

.detalifot{
  height: 110rpx;
  border-top: 1rpx solid #e7e7e7;
  width: 750rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  background-color: #fff;
}
.detalifot .li{
  float: left;
  height: 110rpx;
  width: 33.33%;
  text-align: center;
  font-size: 28rpx;
}
.detalifot .li image{
  width: 36rpx;
  height: 36rpx;
  margin: 15rpx 0 5rpx 0;
}
.detalifot .li:nth-child(3n-1){
  background-color: #70c745;
  line-height: 110rpx;
  color: #fff;
}
.detalifot .li:nth-child(3n){
  background-color: #11a8e1;
  line-height: 110rpx;
  color: #fff;
}

.detalisCon{
  padding-bottom: 111rpx;
}
.detalisCon .tit{
  height: 100rpx;
  line-height: 110rpx;
  background-color: #f6f6f6;
  color: #333;
  text-align: center;
  font-size: 30rpx;
}

.layerColor{
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.7);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 999;
}
.layerColor .box{
  background-color: #fff;
  border-radius: 20rpx 20rpx 0 0rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
}
.layerColor .box .sp{
  height: 170rpx;
  padding: 0 30rpx;
}
.layerColor .box .sp .img{
  width: 200rpx;
  height: 170rpx;
  float: left;
}
.layerColor .box .sp .img image{
  width: 200rpx;
  height: 200rpx;
  margin-top: -30rpx;
}
.layerColor .box .sp .xq{
  width: 400rpx;
  height: 170rpx;
  float: left;
  margin-left: 30rpx;
}
.layerColor .box .sp .xq .t:first-child{
  font-size: 30rpx;
  color: #ff7979;
  margin-top: 12rpx;
}
.layerColor .box .sp .xq .t{
  font-size: 28rpx;
  color: #333;
  height: 50rpx;
  line-height: 50rpx;
}
.layerColor .box .sp .close image{
  width: 30rpx;
  height: 30rpx;
  margin-top: 30rpx;
  display: block;
  float: right;
}
.layerColor .box .bt{
  height: 50rpx;
  line-height: 50rpx;
  padding: 0 30rpx;
  font-size: 28rpx;
  margin-top: 20rpx;
}
.layerColor .box .ul{
  padding: 0 30rpx;
  overflow: hidden;
}
.layerColor .box .ul .li{
  float: left;
  width: 150rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 5rpx;
  font-size: 28rpx;
  border: 1px solid #e7e7e7;
  margin-left: 25rpx;
  margin-top: 20rpx;
}
.layerColor .box .ul .li:nth-child(4n-3){
  margin-left: 0;
}
.layerColor .box .ul .select{
  background-color: #11a8e1;
  color: #fff;
}
.layerColor .box .sure{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  text-align: center;
  background-color: #11a8e1;
  margin-top: 20rpx;
  color: #fff;
}

.layerLiftcar{
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.7);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 999;
}
.layerLiftcar .box{
  background-color: #fff;
  border-radius: 20rpx 20rpx 0 0rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
}
.layerLiftcar .box .top{
  padding: 0 30rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #333;
  text-align: center;
}
.layerLiftcar .box .top .img{
  position: absolute;
  right: 30rpx;
  top: 30rpx;
  width: 30rpx;
  height: 30rpx;
}
.layerLiftcar .box .top .img image{
  display: block;
  width: 30rpx;
  height: 30rpx;
}
.layerLiftcar .box .ul{
  padding: 0 30rpx 30rpx;
}
.layerLiftcar .box .ul .li{
  margin-top: 60rpx;
  overflow: hidden
}
.layerLiftcar .box .ul .li .img{
  height: 115rpx;
  width: 115rpx;
  float: left;
}
.layerLiftcar .box .ul .li .img image{
  height: 115rpx;
  width: 115rpx;
}
.layerLiftcar .box .ul .li .text{
  width: 530rpx;
  height: 115rpx;
  float: right;
}
.layerLiftcar .box .ul .li .text .t{
  color: #333;
  font-size: 30rpx;
  height: 50rpx;
  line-height: 50rpx;
}
.layerLiftcar .box .ul .li .text .b{
  font-size: 24rpx;
  line-height: 30rpx;
  color: #767676;
}

.fq{
  padding: 30rpx;
}
.fq .text{
  background-color: #f6f6f6;
  border-radius: 10rpx;
  overflow: hidden;
}
.fq .text .tb text{
  width: 33%;
  float: left;
  display: block;
  text-align: center;
  font-size: 28rpx;
  padding: 30rpx 0 15rpx;
}
.fq .text .wb text{
  width: 33%;
  float: left;
  display: block;
  text-align: center;
  color: #ff7979;
  padding: 15rpx 0 30rpx;
}

.upMore{
  width: 100%;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  color: #333;
  font-size: 28rpx;
  padding-bottom: 100rpx;
}
.upMore image{
  width: 28rpx;
  height: 15rpx;
  vertical-align: middle;
  margin: 0 20rpx 7rpx 0;
}