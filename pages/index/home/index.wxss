.indTop{
  width: 100%;
  height: 60rpx;
  background-color: #11a8e1;
  padding: 0rpx 0 10rpx 0;
}
.indTop .city{
  width: 170rpx;
  height: 60rpx;
  text-align: center;
  font-size: 10px;
  color: #fff;
  cursor: pointer;
  float: left;
  line-height: 60rpx;
}
.indTop .city image{
  width: 22rpx;
  height: 12rpx;
  vertical-align: middle;
  margin: 0 3px 2px 0;
}
.indTop .city button{
  background-color: #11a8e1;
  width: 170rpx;
  height: 60rpx;
  text-align: center;
  font-size: 10px;
  color: #fff;
  cursor: pointer;
  float: left;
  line-height: 60rpx;
  border: 0;
}
.indTop .city .bottom{
  animation:0.3s linear 0s rotate;
  animation-fill-mode:forwards;
  -webkit-transition: 0.3s linear 0s rotate;
  -moz-transition: 0.3s linear 0s rotate;
  -o-transition: 0.3s linear 0s rotate;
}

.indTop .search{
  width: 550rpx;
  height: 60rpx;
  background-color: #3db1dd;
  border-radius: 5px;
  float: left;
}
.indTop .search .button{
  width: 60rpx;
  height: 60rpx;
  position: relative;
  float: left;
}
.indTop .search .button image{
  width: 33rpx;
  height: 33rpx;
  position: absolute;
  right: 0;
  top: 15rpx;
}
.indTop .search .text{
  font-size: 26rpx;
  color: #fff;
  height: 60rpx;
  border: 0;
  line-height: 60rpx;
  width: 450rpx;
  margin-left: 20rpx;
  float: left;
}

.indSwiper{
