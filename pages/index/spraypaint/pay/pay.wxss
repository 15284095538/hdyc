page{
  background-color: #f6f6f6;
}
.l{
  float: left;
}
.r{
  float: right;
}
.none{
  display: none;
}
.block{
  display: block;
}

.payli{
  padding: 0 30rpx;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  font-size: 28rpx;
  position: relative;
  color: #333;
}
.payli .more{
  position: absolute;
  right: 30rpx;
  top: 35rpx;
  width: 17rpx;
  height: 33rpx;
}
.payli .r{
  margin-right: 40rpx;
}
.payli .intr{
  position: absolute;
  right: 0;
  top: 0;
}
.payli .red{
  color: #ff7979;
}
.payli .li{
  height: 99rpx;
  border-bottom: 1rpx solid #e7e7e7;
}
.payli .li input{
  height: 99rpx;
  border:0;
  text-align: right;
}

.paystore{
  height: 185rpx;
  padding: 30rpx;
  background-color: #fff;
  margin-top: 20rpx;
}
.left {
  float: left;
}

.right {
  float: right;
}

.clear {
  clear: both;
}


.list-img {
  width: 210rpx;
  height: 187rpx;
  padding-right: 30rpx;
  float: left;
}

.list-img image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.list-info {
  float: left;
  width: calc(100% - 240rpx);
  margin-top: 10rpx;
}

.list-title {
  height: 30rpx;
}

.list-name {
  font-size: 30rpx;
  line-height: 30rpx;
  color: #333;
}

.list-name-img {
  position: relative;
  width: 35rpx;
  height: 30rpx;
  margin-left: 18rpx;
}

.list-name-img image {
  width: 35rpx;
  height: 30rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

.list-evaluate {
  height: 24rpx;
  margin: 18rpx 0rpx;
}

.list-evaluate-left, .list-evaluate-right {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #aeaeae;
}

.list-evaluate-left text, .list-evaluate-right text {
  color: #ff7979;
}

.list-evaluate-line {
  width: 51rpx;
  height: 24rpx;
  font-size: 24rpx;
  line-height: 24rpx;
  text-align: center;
  color: #c8c8c8;
}

.list-address {
  height: 24rpx;
  font-size: 24rpx;
  line-height: 24rpx;
  color: #aeaeae;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.list-more {
  height: 26rpx;
  margin-top: 20rpx;
}

.list-tag {
  width: 42rpx;
  font-size: 16rpx;
  line-height: 26rpx;
  color: #11a8e1;
  border: 1rpx solid #11a8e1;
  text-align: center;
  border-radius: 6rpx;
  margin-right: 10rpx;
}

.list-distance {
  font-size: 26rpx;
  line-height: 26rpx;
  color: #aeaeae;
}

.fot{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  border-top: 1rpx solid #f6f6f6;
  background-color: #fff;
}
.fot .l{
  float: left;
  width: 450rpx;
  padding-left: 30rpx;
  font-size: 36rpx;
  color: #333;
}
.fot .l text{
  color: #ff7979;
}
.fot .r{
  float: right;
  width: 270rpx;
  background-color: #11a8e1;
  font-size: 36rpx;
  color: #fff;
  text-align: center;
}


.couponlist{
  height: 170rpx;
  overflow: hidden;
  background-color: #fff;
  width: 690rpx;
  margin: 40rpx auto 0;
  position: relative;
}
.couponlist .sImg{
  width: 55rpx;
  height: 52rpx;
  position: absolute;
  right: 0rpx;
  top: 0;
  display: none;
}
.couponlistselect .sImg{
  display: block;
}
.couponlist .left{
  width: 200rpx;
  height: 170rpx;
  line-height: 170rpx;
  font-size:46rpx;
  text-align:center;
  color: #fff;
  background-color:#11a8e1;
  float: left;
}
.couponlist .right{
  height: 170rpx;
  line-height: 170rpx;
  width: 410rpx;
  padding-left: 50rpx;
  float: left;
  font-size:30rpx;
  color:#333;
}
.layer{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9;
  background-color: #fff;
}