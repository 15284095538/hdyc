page{
  background-color: #f6f6f6;
  color: #333;
  font-size: 30rpx;
}
.none{
  display: none;
}
.block{
  display: block;
}

.orderpay{
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  position: relative;
}
.orderpay .li{
  height: 99rpx;
  line-height: 99rpx;
  border-bottom: 1rpx solid #e7e7e7;
  padding: 0 30rpx;
}
.orderpay .li input{
  width: 380rpx;
  height: 99rpx;
  line-height: 99rpx;
  position: absolute;
  right: 30rpx;
  top: 0;
  text-align: right;
}
.orderpay .li .adr{
  width: 310rpx;
  height: 99rpx;
  line-height: 99rpx;
  position: absolute;
  left: 185rpx;
  top: 0;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.orderpay .li .adrdz{
  width: 310rpx;
  height: 99rpx;
  line-height: 99rpx;
  position: absolute;
  right: 80rpx;
  text-align: right;
  top: 0;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.orderpay .li .pri{
  height: 99rpx;
  line-height: 99rpx;
  position: absolute;
  right: 30rpx;
  text-align: right;
  top: 0;
  color: #ff7979;
}
.orderpay .li .km{
  width: 130rpx;
  height: 99rpx;
  line-height: 99rpx;
  position: absolute;
  right: 80rpx;
  top: 0;
  text-align: right;
}
.orderpay .li .more{
  width: 17rpx;
  height: 33rpx;
  position: absolute;
  right: 30rpx;
  top: 50%;
  margin-top: -16.5rpx;
}

.orderpaySp{
  margin-top: 20rpx;
  background-color: #fff;
  overflow: hidden;
}
.orderpaySp .li{
  padding: 30rpx;
  position: relative;
  height: 127rpx;
}
.orderpaySp .li .img{
  width: 465rpx;
  height: 127rpx;
  white-space: nowrap;
  display: flex;
}
.orderpaySp .li .img view{
  display: inline-block;
  width: 125rpx;
  height: 127rpx;
  margin-right: 30rpx;
}
.orderpaySp .li .img imag:first-child{
  margin-left: 0;
}
.orderpaySp .li .img image{
  width: 123rpx;
  height: 123rpx;
  border: 1rpx solid #e7e7e7;
}
.orderpaySp .li .sp{
  position: absolute;
  right: 80rpx;
  height: 127rpx;
  line-height: 127rpx;
  top: 30rpx;
}
.orderpaySp .li .more{
  width: 17rpx;
  height: 33rpx;
  position: absolute;
  right: 30rpx;
  top: 50%;
  margin-top: -16.5rpx;
}

.fot{
  width: 100%;
  height: 110rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 2;
  background-color: #fff;
}
.fot .left{
  width: 450rpx;
  padding: 0 0 0 30rpx;
  height: 110rpx;
  line-height: 110rpx;
  float: left;
}
.fot .left text{
  color: #ff7979;
}
.fot .right{
  width: 270rpx;
  background-color: #11a8e1;
  text-align: center;
  height: 110rpx;
  line-height: 110rpx;
  font-size: 32rpx;
  float: right;
  color: #fff;
}

.layer{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9;
  background-color: #fff;
}


.washlist .li{
  height: 185rpx;
  padding: 30rpx 30rpx;
  border-bottom: 20rpx solid #f6f6f6;
}
.washlist .li .img{
  width: 210rpx;
  height: 185rpx;
  position: relative;
  border-radius: 10rpx;
  overflow: hidden;
  float: left;
}
.washlist .li .img image{
  position: absolute;
  left: 0;
  top: 0;
  width: 210rpx;
  height: 185rpx;
}
.washlist .li .img .t{
  height: 40rpx;
  background-color: rgba(0, 0, 0, 0.5);
  width: 210rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  color: #fff;
  text-align: center;
  line-height: 40rpx;
  font-size: 24rpx;
}
.washlist .li .text{
  width: 450rpx;
  height: 185rpx;
  float: right;
}
.washlist .li .text .name{
  height: 50rpx;
  font-size: 30rpx;
  line-height: 30rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  color: #333333;
}
.washlist .li .text .name image{
  width: 35rpx;
  height: 29rpx;
  vertical-align: middle;
  margin: 0 0 5rpx 10rpx;
}
.washlist .li .text .num{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  color: #aeaeae;
}
.washlist .li .text .num text{
  color: #ff7979;
  font-weight: bold;
  margin: 0 5rpx;
}
.washlist .li .text .address{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  color: #aeaeae;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.washlist .li .text .bot{
  height: 40rpx;
  font-size: 24rpx;
  line-height: 40rpx;
}
.washlist .li .text .bot .l{
  width: 360rpx;
  float: left;
}
.washlist .li .text .bot .l text{
  border: 1px solid #11a8e1;
  color: #11a8e1;
  padding: 3rpx 5rpx;
  margin-right: 10rpx;
  border-radius: 5rpx;
}
.washlist .li .text .bot .r{
  float: right;
  color: #aeaeae;
}


.spImg{
  padding: 0 30rpx;
  height: 100%;
}
.spImg .li{
  border-bottom: 1rpx solid #e7e7e7;
  height: 110rpx;
  padding: 20rpx 0;
  position: relative;
}
.spImg .li .img{
  width: 110rpx;
  height: 110rpx;
  float: left;
}
.spImg .li .img image{
  width: 110rpx;
  height: 110rpx;
}
.spImg .li .t{
  width: 550rpx;
  height: 60rpx;
  line-height: 30rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  float: right;
  font-size: 26rpx;
}
.spImg .li .p{
  width: 550rpx;
  height: 30rpx;
  line-height: 30rpx;
  float: right;
  color: #ff7979;
  margin-top: 20rpx;
}
.spImg .num{
  position: absolute;
  right: 0;
  bottom:20rpx;
  color: #ff7979;
}

.couponlist{
  height: 170rpx;
  overflow: hidden;
  background-color: #fff;
  width: 690rpx;
  margin: 40rpx auto 0;
  position: relative;
}
.couponlist .sImg{
  width: 55rpx;
  height: 52rpx;
  position: absolute;
  right: 0rpx;
  top: 0;
  display: none;
}
.couponlistselect .sImg{
  display: block;
}
.couponlist .left{
  width: 200rpx;
  height: 170rpx;
  line-height: 170rpx;
  font-size:46rpx;
  text-align:center;
  color: #fff;
  background-color:#11a8e1;
  float: left;
}
.couponlist .right{
  height: 170rpx;
  line-height: 170rpx;
  width: 410rpx;
  padding-left: 50rpx;
  float: left;
  font-size:30rpx;
  color:#333;
}