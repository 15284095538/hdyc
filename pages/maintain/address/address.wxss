page{
  background: #f2f2f2;
}
.list{
  box-sizing: border-box;
  padding: 18rpx;
  background: white;
  margin-bottom: 20rpx;
  display: flex;justify-content: space-between;
}
.img{
  width: 180rpx;
  height: 180rpx;
  margin-right: 20rpx;
}
.img image{
  width: 100%;
  height: 100%;
}
.company{
  width: 510rpx;
  min-height: 180rpx;
}
.company>view{
  width: 100%;
}
.name{
  font-size: 30rpx;
}
.score,.address{
  font-size: 24rpx;
  color: #666;
  overflow: hidden;
  min-height: 46rpx;
}
.score text{
  color: red;
}
.score>view{
  width: 160rpx;float: left; line-height: 50rpx;
}
.tag{
  float: left;
}
.tag text{
  border: 1px solid rgb(17,168,225);
  border-radius: 10rpx;color: rgb(17,168,225);
  font-size: 20rpx;margin-right: 20rpx;padding: 4rpx 8rpx;
}
.distance{
  float: right;
  min-height: 50rpx;
  font-size: 24rpx;
  color: #666;padding-top: 20rpx;
}