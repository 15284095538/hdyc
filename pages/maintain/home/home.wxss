.sure{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #11a8e1;
  color: #fff;
  text-align: center;
  font-size: 30rpx;
  margin-top: 20rpx;
}

.top{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  position: fixed;
  left: 0;
  top: 0;
  display: flex;
  background-color: #fff;
  z-index: 3;
}
.top .li{
  width: 50%;
  height: 80rpx;
  line-height: 80rpx;
  display: inline-block;
  font-size: 28rpx;
  text-align: center;
}
.top .select{
  color: #11a8e1;
  border-bottom: 5rpx solid #11a8e1;
}
