page{
  background:#f2f2f2;
}
.none{
  display: none;
}
.block{
  display: block;
}
.box{
  padding-bottom: 160rpx;
}
.chooseCar{
  margin-top:80rpx;
  font-size: 26rpx;
  line-height: 80rpx;
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;padding: 0 20rpx;
  background: white;
}
.title{
  font-size: 28rpx;
  line-height: 80rpx;
  background: transparent;
  box-sizing: border-box;
  padding:0 20rpx;
}
.box .list{
  background: white;
  font-size: 26rpx;
  box-sizing: border-box;
  padding:0 20rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.list .listone{
  overflow: hidden;
}
.list .listone view{
  height: 80rpx;
  
}
.close{
  float: left;margin-right:20rpx;
}
.close span{
  display: inline-block;width: 0;height: 100%;vertical-align: middle;
}
.close .icon{
  display: inline-block;vertical-align: middle;
}
.typename{
  float: left;
  line-height: 80rpx;
}
.handle{
  float: right;
  line-height: 80rpx;
}
.arrow{
  float: right;
  line-height: 80rpx;
  width: 60rpx;
  text-align: right;
}
.arrow image{
  width: 26rpx;
  height: 26rpx;
  margin-top: 26rpx;
}







.list .listtwo{
  padding: 20rpx 0;
  height: 160rpx;
  box-sizing: border-box;padding-left: 40rpx;
}
.list .listtwo .end{
  height: 120rpx;
  float: left;
}
.list .listtwo .end>view{
  margin: 0 20rpx;
}
.list .listtwo .con{
  display: flex;
  width: 690rpx;
  height: 140rpx;
  white-space:nowrap;
}
.list .listtwo .checkbox{
  width: 70rpx;
  height: 140rpx;
  position: relative;
  display: inline-block;
}
.list .listtwo .checkbox image{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  left: 0;
  top: 50rpx;
}
.list .listtwo .img{
  width: 120rpx;
  height: 120rpx;
  border: 1rpx solid #e6e6e6;
  float: left;margin:0 20rpx;
}
.list .listtwo .img image{
  width: 120rpx;
  height: 120rpx;
}
.list .listtwo .num{
  float: left;
  height: 120rpx;
  padding-top: 24rpx;
}
.list .listtwo .hide{
  width: 140rpx;
  height: 140rpx;
  float: left;
  vertical-align: top;
  text-align: center;
  line-height: 40rpx;
  color: #fff;
}
.list .listtwo .hide text{
  display: block;
  margin: 30rpx auto;
}
.list .listtwo .delete,
.list .listtwo .collection{
  padding-top: 24rpx;
  height: 120rpx;
  width: 80rpx;
  float: left;
  font-size: 24rpx;
}
.list .listtwo .delete image,
.list .listtwo .collection image{
  display: block;margin: auto;
  width: 40rpx;
  height: 40rpx;
  margin-bottom: 10rpx;
}
.list .listtwo .delete text,
.list .listtwo .collection text{
  text-align: center;
  display: block;float: left;
  width: 100%;
}

.listtwo .start{
  overflow: hidden;
}
.listtwo .start .start-con{
  float: left;
  width: 370rpx;
  height: 120rpx;
  overflow: hidden;
}
.listtwo .start .start-name,
.listtwo .start .start-price{
  float: left;
  width: 100%;
  height: 60rpx;
}
.listtwo .start .start-price{
  line-height: 90rpx;
  color: red;
}
.listtwo .start .start-num{
  float: right;margin-right:20rpx;
  height: 120rpx;
  line-height: 210rpx;
}

















.Settlement{
  height: 90rpx;
  border-top: 1rpx solid #e0e0e0;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  background-color: #fff;
  padding: 0 0rpx 0 20rpx;
}
.Settlement .gobuy{
  float: right;
  width: 220rpx;
  background-color: rgb(17,168,225);
  color: #fff;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 26rpx;
}
.Settlement .allprice{
  float: left;
  margin-right: 20rpx;
  height: 100rpx;
  font-size: 24rpx;
  box-sizing: border-box;padding: 10 0rpx;
}
.Settlement .allprice .all1{
  float: left;
  width: 100%;
  line-height: 50rpx;
}
.Settlement .allprice .all2{
  float: left;
  width: 100%;
  line-height: 40rpx;
  color: #666;
}
.Settlement .allprice text{
  color: #f56600;
  font-size: 26rpx;
}




.stepper {
	width: 180rpx;
	height: 60rpx;
	border: 1rpx solid #f2f2f2;
	border-radius: 20px;
	margin: 10rpx 0;
}
.stepper text {
	width: 48rpx;
	line-height: 60rpx;
	text-align: center;
	float: left;
}
.stepper input {
	width: 80rpx;
	height: 60rpx;
  min-height: 60rpx;
	float: left;
	text-align: center;
	font-size: 12px;
	border-left: 1rpx solid #f2f2f2;
	border-right: 1rpx solid #f2f2f2;
}
.stepper .normal{
	color: black;
}
.stepper .disabled{
	color: #ccc;
}

