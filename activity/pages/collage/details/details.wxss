page{
  color: #333333;
  background-color: #f6f6f6;
}

.detTop{
  width: 750rpx;
  height: 370rpx;
  background-color: #ff9756;
  padding: 50rpx 30rpx 0;
  position: relative;
}
.detTop .det{
  width: 200rpx;
  height: 160rpx;
  background-color: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}
.detTop .det .t{
  font-size: 26rpx;
  color: #fb6e14;
  text-align: center;
  margin-top: 30rpx;
}
.detTop .det .t text{
  font-size: 38rpx;
}
.detTop .det .b{
  font-size: 26rpx;
  margin-top: 20rpx;
  text-align: center;
}
.detTop .ts{
  color: #fff;
  font-size: 30rpx;
  position: absolute;
  left: 270rpx;
  top: 90rpx;
}

.countdown{
  width: 275rpx;
  height: 50rpx;
  line-height: 50rpx;
  position: absolute;
  top: 150rpx;
  left: 270rpx;
  color: #fff;
  font-size: 28rpx;
}
.countdown text{
  float: left;
}
.countdown .t{
  display: block;
  width: 50rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin: 0 5rpx;
  color: #ffa546;
}



.detYq{
  width: 690rpx;
  margin: -130rpx auto 0;
  border-radius: 30rpx;
  position: relative;
  z-index: 2;
  background-color: #fff;
  box-shadow: 0rpx 5rpx 20rpx #e1e1e1;
  padding-bottom: 40rpx;
}
.detYq .tit{
  height: 130rpx;
  line-height: 130rpx;
  text-align: center;
  font-size: 34rpx;
}
.detYq .tit text{
  color: #fb6e14;
}
.detYq .head{
  width: 100%;
  overflow: hidden;
  padding-bottom: 40rpx;
}
.detYq .head .li{
  width: 102rpx;
  height: 150rpx;
  float: left;
  position: relative;
  margin-left: 28rpx;
  margin-top: 40rpx;
  text-align: center;
  font-size: 20rpx;
}
.detYq .head .li image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  border: 1rpx solid #d4d4d4;
}
.detYq .head .li .t{
  width: 60rpx;
  height: 30rpx;
  position: absolute;
  left: 20rpx;
  bottom: 40rpx;
  background-color: #fb6e14;
  color: #fff;
  font-size: 20rpx;
  text-align: center;
  border-radius: 20rpx;
  line-height: 30rpx;
}
.detYq .head .li .u{
  height: 50rpx;
  line-height: 50rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;  
}
.detYq .head .btn{
  width: 100rpx;
  height: 100rpx;
  float: left;
  position: relative;
  margin-left: 28rpx;
  margin-top: 40rpx;
  text-align: center;
  font-size: 20rpx;
  line-height: 100rpx;
  border-radius: 50%;
}
.detYq .sure{
  width: 620rpx;
  height: 90rpx;
  background-color: #fb6e14;
  color: #fff;
  font-size: 36rpx;
  margin: 0 auto;
  text-align: center;
  border-radius: 30rpx;
  line-height: 90rpx;
}